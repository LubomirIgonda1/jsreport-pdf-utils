{
  "name": "jsreport-pdf-utils",
  "version": "0.1.0",
  "description": "jsreport extension providing pdf operations like merge or concatenation",
  "main": "index.js",
  "scripts": {
    "test": "set DEBUG=jsreport && mocha test && standard",
    "start": "jsreport-studio-start --httpPort 5488",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://github.com/jsreport/jsreport-pdf-utils",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-pdf-utils.git"
  },
  "license": "MIT",
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.0.2",
    "eslint": "4.11.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "in-publish": "2.0.0",
    "jsreport-chrome-pdf": "0.1.0",
    "jsreport-core": "1.5.1",
    "jsreport-studio-dev": "1.1.1",
    "jsreport-templates": "1.2.2",
    "mocha": "4.0.1",   
    "rimraf": "2.6.2",
    "should": "13.1.3",
    "standard": "10.0.3"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "pdfjs": "2.0.0-alpha.3",
    "pdf2json": "1.1.7"
  },
  "files": [
    "lib",
    "studio",
    "test",
    "static",
    "index.js",
    "jsreport.config.js"
  ]
}
