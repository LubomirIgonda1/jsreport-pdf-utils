{
  "name": "jsreport-pdf-utils",
  "version": "1.0.3",
  "description": "jsreport extension providing pdf operations like merge or concatenation",
  "main": "index.js",
  "scripts": {
    "test": "set DEBUG=jsreport && mocha test --timeout=20000 && standard",
    "start": "jsreport-studio-start --httpPort 5488",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=8.9"
  },
  "homepage": "https://github.com/jsreport/jsreport-pdf-utils",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-pdf-utils.git"
  },
  "license": "MIT",
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.18.2",
    "eslint-config-standard-jsx": "5.0.0",
    "eslint-plugin-babel": "4.1.2",
    "in-publish": "2.0.0",
    "jsreport-chrome-pdf": "1.1.5",
    "jsreport-core": "2.1.0",
    "jsreport-handlebars": "2.0.0",
    "jsreport-jsrender": "2.0.0",
    "jsreport-studio-dev": "1.3.0",
    "jsreport-templates": "2.0.0",
    "mocha": "5.2.0",
    "puppeteer": "1.5.0",
    "rimraf": "2.6.2",
    "should": "13.2.1",
    "standard": "11.0.0"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "jsreport-pdfjs": "0.2.0",
    "pdfjs-dist": "2.0.457"
  },
  "files": [
    "lib",
    "studio",
    "test",
    "static",
    "index.js",
    "jsreport.config.js"
  ]
}
